alias cbat="bat --paging never"
alias d8="rlwrap $CODE_PATH/v8/out.gn/x64.debug/d8 --allow-natives-syntax"
alias ga="git add"
alias gaa="git add -A"
alias gc="git commit"
alias gcm="git commit -m"
alias gco="git checkout"
alias gcleanup-merged="git branch --merged | egrep -v '(^\*|master|main|dev|develop)' | xargs git branch -d"
alias gcleanup-squash-merged="git-list-squash-merged | xargs git branch -D"
alias gcleanup="gcleanup-merged && gcleanup-squash-merged"
alias gdiff="git diff -- ':!package-lock.json' ':!yarn.lock'"
alias gce="gh copilot explain"
alias gcs="gh copilot suggest"
alias gm="git merge"
alias gpull="git pull"
alias gpush="git push"
alias gst="git status"
alias gsw="git switch"
alias less="bat --paging always"
alias lg="lazygit"
alias ls="ls -Ga"
alias oc="overmind connect"
alias or="overmind restart"
alias os="overmind start"
alias osl="overmind start -l"
alias vim="nvim"

alias td="bundle && yarn && rails db:migrate && rails tmp:cache:clear && rails i18n:js:export"